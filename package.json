{
  "name": "@eso-status/api",
  "version": "2.0.0-dev.4",
  "description": "Eso-status api sources code",
  "author": "@dov118 <contact@dov118.dev> (https://dov118.dev)",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "20.17.0"
  },
  "bugs": {
    "url": "https://github.com/eso-status/api/issues"
  },
  "homepage": "https://github.com/eso-status/api#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:eso-status/api.git"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --setupFiles",
    "test:watch": "jest --runInBand --watch --setupFiles",
    "test:cov": "jest --runInBand --coverage --setupFiles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --setupFiles",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json --setupFiles",
    "migration:run": "typeorm migration:run -d ./dist/config/typeorm.config.js"
  },
  "dependencies": {
    "@eso-status/forum-message": "2.0.0-dev.6",
    "@eso-status/live-services": "2.0.0-dev.2",
    "@eso-status/service-alerts": "2.0.0-dev.2",
    "@eso-status/types": "2.0.0-dev.12",
    "@nestjs/cli": "10.4.5",
    "@nestjs/common": "10.4.4",
    "@nestjs/core": "10.4.4",
    "@nestjs/platform-express": "10.4.4",
    "@nestjs/platform-socket.io": "10.4.4",
    "@nestjs/schedule": "4.1.1",
    "@nestjs/testing": "10.4.4",
    "@nestjs/typeorm": "10.0.2",
    "@nestjs/websockets": "10.4.4",
    "dotenv": "16.4.5",
    "moment": "2.30.1",
    "mysql2": "3.11.3",
    "reflect-metadata": "0.2.2",
    "socket.io": "4.8.0",
    "socket.io-client": "4.8.0",
    "sqlite3": "5.1.7",
    "supertest": "7.0.0",
    "typeorm": "0.3.20",
    "typeorm-extension": "3.6.1",
    "winston": "3.14.2",
    "winston-cloudwatch": "6.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.1.4",
    "@types/jest": "29.5.13",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sonarjs": "1.0.4",
    "eslint-plugin-unused-imports": "4.1.4",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.e2e-spec\\.ts|.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
