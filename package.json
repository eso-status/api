{
  "name": "@eso-status/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "DOTENV_CONFIG_PATH=.env.test jest --runInBand --setupFiles dotenv/config",
    "test:watch": "DOTENV_CONFIG_PATH=.env.test jest --runInBand --watch --setupFiles dotenv/config",
    "test:cov": "DOTENV_CONFIG_PATH=.env.test jest --runInBand --coverage --setupFiles dotenv/config",
    "test:debug": "DOTENV_CONFIG_PATH=.env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --setupFiles dotenv/config",
    "test:e2e": "DOTENV_CONFIG_PATH=.env.test jest --runInBand --config ./test/jest-e2e.json --setupFiles dotenv/config",
    "seed": "DOTENV_CONFIG_PATH=.env.test npm run build && node dist/database/seeds/seed.js",
    "migration:run": "DOTENV_CONFIG_PATH=.env.test npm run build && typeorm migration:run -d ./dist/config/typeorm.config.js"
  },
  "dependencies": {
    "@eso-status/forum-message": "1.0.8",
    "@eso-status/live-services": "1.0.6",
    "@eso-status/service-alerts": "1.0.6",
    "@eso-status/types": "1.0.6",
    "@nestjs/common": "10.3.10",
    "@nestjs/core": "10.3.10",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/platform-socket.io": "10.3.10",
    "@nestjs/schedule": "4.1.0",
    "@nestjs/typeorm": "10.0.2",
    "@nestjs/websockets": "10.3.10",
    "aws-sdk": "2.1659.0",
    "moment": "2.30.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "socket.io": "4.7.5",
    "socket.io-client": "4.7.5",
    "sqlite3": "5.1.7",
    "typeorm": "0.3.20",
    "typeorm-extension": "3.5.1",
    "winston": "3.13.1",
    "winston-cloudwatch": "6.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.2",
    "@nestjs/testing": "10.3.10",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.11",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.e2e-spec\\.ts|.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
